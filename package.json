{
  "name": "babel-plugin-import-recursive",
  "version": "1.1.3",
  "description": "A babel plugin that unrolls wildcard imports.",
  "main": "lib/index.js",
  "files": [
    "lib/index.js"
  ],
  "keywords": [
    "babel",
    "import",
    "recursive",
    "wildcard"
  ],
  "scripts": {
    "test": "jest",
    "build": "rimraf lib && babel -d ./lib ./src --ignore ./src/test",
    "prepare": "rimraf lib && babel -d ./lib ./src --ignore ./src/test",
    "lint": "eslint",
    "prettify": "prettier"
  },
  "dependencies": {
    "@babel/template": "^7.12.13"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/preset-env": "7.14.8",
    "@babel/register": "7.14.5",
    "eslint": "7.31.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "jest": "27.0.6",
    "lint-staged": "11.0.0",
    "prettier": "2.3.2",
    "rimraf": "3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": "https://github.com/dromru/babel-plugin-import-recursive.git",
  "author": "dromru",
  "license": "MIT"
}
